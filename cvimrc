" https://github.com/1995eaton/chromium-vim

let mapleader = ","
let searchalias g = "google"
set noautofocus
let barposition = "bottom"

map j scrollPageDown
map k scrollPageUp
map d scrollDown
map u scrollUp
map l nextTab
map h previousTab
map b scroll left
map w scroll right
map ; openCommandBar
map <C-b> :bookmarks<CR>
map <C-w> :quit<CR>

map <Leader>b :buffer<Space>
map <Leader>h :history<Space>
map <Leader>th :tabhistory<Space>

map <Leader>gh :open https://github.com<CR>
map <Leader>fv :open https://favstar.fm/me<CR>
map <Leader>tw :open https://twitter.com/i/notifications<CR>

map gn :open chrome://newtab<CR>

" vim:ft=vim:
